{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": [
    "package.json",
    "define.config.ts",
    "vite.config.ts",
    "vite.firefox.config.ts",
    "manifest.config.ts",
    "manifest.firefox.config.ts"
  ]
}
